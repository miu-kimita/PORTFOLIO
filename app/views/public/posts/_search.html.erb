<div class="name-search">
    <h5><%= t('dictionary.button.search') %></h5>
    <%= form_with url: search_path, method: :get, local: true do |f| %>
    <% end %>
</div>
<table class="table table-bordered genre-table">
    <thead>
      <tr>
        <th><%= t('dictionary.button.search') %></th>
      </tr>
    </thead>
    <tbody>
    <% genre.each do |genre| %>
      <tr>
        <td>
          <%= link_to genre.genre_name, search_path('search[value]': genre.id, 'search[how]': "match") %>
        </td>
      </tr>
    <% end %>
    </tbody>
</table>